<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="max_collapsed_lines" format="integer"/>
        <attr name="expand_drawable" format="reference"/>
        <attr name="collapse_drawable" format="reference"/>
        <attr name="need_drawable" format="boolean"/>
        <attr name="need_text" format="boolean"/>
        <attr name="expand_text" format="reference|string"/>
        <attr name="collapse_text" format="reference|string"/>
        <attr name="only_expand" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ExpandableTextView2">
        <attr name="tv_expandLines" format="integer" />
        <attr name="tv_shrinkBitmap" format="reference" />
        <attr name="tv_expandBitmap" format="reference" />
        <attr name="tv_textStateColor" format="color" />
        <attr name="tv_textContentColor" format="color" />
        <attr name="tv_textContentSize" format="dimension" />
        <attr name="tv_textShrink" format="string" />
        <attr name="tv_textExpand" format="string" />
    </declare-styleable>
</resources>